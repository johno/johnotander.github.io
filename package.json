{
  "name": "johnotander.com",
  "version": "0.0.0",
  "description": "I build things by banging on a keyboard",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "node build/config && npm run build:jekyll && npm run build:css && npm run build:uncss && npm run build:minify",
    "build:css": "tachyons src/css/all.css > src/css/c.css",
    "build:jekyll": "jekyll build",
    "build:minify": "tachyons public/css/c.css > public/css/c.min.css --minify",
    "build:uncss": "node build/uncss"
  },
  "repository": "johnotander/johnotander.github.io",
  "author": "John Otander",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "glob": "^7.1.1",
    "johno-api": "^1.5.0",
    "js-yaml": "^3.7.0",
    "tachyons": "^4.6.1",
    "tachyons-cli": "^1.0.10",
    "uncss": "^0.14.1"
  }
}
